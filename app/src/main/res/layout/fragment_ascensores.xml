                    android:layout_marginStart="@dimen/distance_text_ascensores"
                    android:layout_marginBottom="32dp"
                    android:text="Tiempo promedio"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/destino2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/distance_text_ascensores"
                    android:layout_marginTop="21dp"
                    android:layout_marginBottom="16dp"
                    android:text="Destino"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toTopOf="@+id/tp2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/origen2" />

                <TextView
                    android:id="@+id/infodestino2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/t2"
                    app:layout_constraintTop_toTopOf="@+id/destino2" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
