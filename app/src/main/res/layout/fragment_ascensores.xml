                <TextView
                    android:id="@+id/destino3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/distance_text_ascensores"
                    android:layout_marginTop="21dp"
                    android:layout_marginBottom="16dp"
                    android:text="Destino"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toTopOf="@+id/tp3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/origen3" />

                <TextView
                    android:id="@+id/infodestino3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="12"
