                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/distance_text_ascensores"
                    android:layout_marginBottom="32dp"
                    android:text="Tiempo promedio"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/destino1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/distance_text_ascensores"
                    android:layout_marginTop="21dp"
                    android:layout_marginBottom="16dp"
                    android:text="Destino"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toTopOf="@+id/tp1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/origen1" />

                <TextView
                    android:id="@+id/infodestino1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cualquiera"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/t1"
                    app:layout_constraintTop_toTopOf="@+id/destino1" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Ascensor 1"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:background="@color/img_selected"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
